//作为第一次上传的简易代码
#include<stdio.h>
int Max(int x,int y)
{
    if(x>y)
    return x；
    else 
    return y;
    }
int main()
{
    int a,b,c;
    scanf("%d\n",a);
    b=((a*100)^99)/(a^99);
    c=Max(a,b);
    printf("The unmber is %d\n",c);
    return0;
}

