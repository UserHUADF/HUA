//按照大小输出数字（此处以输入三个数字为例子）
#include<stdio.h>
int main()
{
	int a=0;
	int b=0;
	int c=0;
	scanf("%d,%d,%d",&a,&b,&c);
	if(a<b)
	{
		int tmp=a;
		a=b;
		b=tmp;
	}
	if(b<c)
	{
		int tmp=b;
		b=c;
		c=tmp;
	}
	if(a<c)
	{
		int tmp=c;
		a=c;
		c=tmp;
	}
	printf("%d %d %d\n",a,b,c);

}


写一个代码打印1-100所有三的倍数
#include<stdio.h>

int main()
{
	int i=0;
	for(i=1;i<=100;i++)
	{
		if(i%3==0)
			printf("%d\n",i);
	}
	return 0;
}
